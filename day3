# taking dyanamic inputs

list1=list(map(int,input("enter the element:").split()))
list2=list(map(int,input("enter the element:").split()))
list=list1+list2
list.sort()
print(list)

list=['hello',12,423,53,0]
list[-1],list[0]=list[0],list[-1]
print(list)

# removing the duplicate elem
list=[1,3,1,2,3,4,5,3,1,2]
list1=[]
for i in list:
    if i not in list1:
        list1.append(i)
print(list1)

# print the odd number of elem
list=[1,3,5,2,4,3,1,5,2]
list1=[]
for i in list:
    if list.count(i)%2!=0 and i not in list1:
        list1.append(i)
print(list1)


# print any 3 elem
list=[8,9,1,2,3,45,0]
list.sort()
print(sum(list[:3]))


# print even nos in desc to acen and then odd nos in acen to desc
list=[12,5,6,9,10,2,1,3,5,5,9,11]
even=[]
odd=[]
for i in list:
    if i%2==0:
        even.append(i)
    else:
        odd.append(i)
even.sort(reverse=True)
print(even)
odd.sort()
print(odd)
res=even+odd
print(res)


list=[12,5,6,9,10,2,1,3,5,5,9,11]
even=[]
odd=[]
for i in list:
    if i%2==0:
        even.append(i)
    else:
        odd.append(i)
even.sort(reverse=True)
odd.sort()
res=even+odd
print(res)


list=[3,5,1,2,8,6,4,7]
list.sort()
list1=[]
for i in list:
    if i%2==0:
        list1.insert(0,i)
    else:
        list1.append(i)
print(list1)


