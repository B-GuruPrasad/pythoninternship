n=int(input())
count=0
while n!=0:
    count+=1
    n=n//10
print(sum)
for i in range(2,(n//2)+1):
    if n%i==0:
        print("not prime")
        break
else:
    print("prime")

num=int(input("enter value:"))
temp1=num
temp2=num
c=0
while temp1>0:
    c+=1
    temp1=temp2//10
arm=0
while temp2>0:
    d=temp2%10
    arm=arm+d**c
    temp2=temp2//10
if arm==num:
    print("Armstrong")
else:
    print("not Armstrong")

# neon
sum=0
num=int(input("enter value:"))
a=num*num
while a>0:
     d=a%10
     sum+=d
     a//=10
if num==sum:
     print("neon")
else:
     print("not a neon")

# magic number
num=int(input("enter value:"))
while num>9:
    sum=0
    while num>0:
        d=num%10
        sum+=d
        num//=10
    num=sum
if num==1:
    print("magic")
else:
    print("not magic")

# composite number
num=int(input("enter value:"))
c=0
for i in range (1,num+1):
    if num%i==0:
        c+=1
if c>2:
    print("composite")
else:
    print("not a composite")

# perfect number
num=int(input("enter value:"))
c=0
for i in range(1,num):
    if num%i==0:
        c+=1
if c==num:
    print("perfect")
else:
    print("not perfect")

# niven number
num=int(input("enter value:"))
sum=0
while num>0:
    d=num%10
    sum+=d
    num//=10
if num%sum==0:
    print("niven number")
else:
    print("not niven")

num=int(input("enter number:"))
org_num=num
fact=1
sum=0
while num>0:
    d=num%10
    for i in range(1,d+1):
        fact=fact*i
    sum=sum+fact
    num=num//10
    fact=1
if org_num==sum:
    print("strong")
else:
    print("not strong")

# spy number
#strong number
num=int(input("enter number:"))
sum=0
mul=1
while num>0:
    d=num%10
    sum=sum+d
    mul*=d
    num=num//10
if sum==mul:
    print("spy number")
else:
    print("not spy num")

