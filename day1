n=int(input("enetr the n values:"))
for i in range(n+1):
    print((n-i)*" ",end=" ")
    print(i*"*")

n=int(input("enetr the n values:"))
for i in range(n,0,-1):
    
    print(i*"*")

n=int(input("enetr the n values:"))
for i in range(n+1):
    for j in range(n-i):
        print(" ",end=" ")
    for j in range(i):
        print("*",end=" ")
    print(" ")

n=int(input("enetr the n values:"))
for i in range(n,0,-1):
    print((n-i)*" ",end=" ")
    print(i*"*")

n=int(input("enetr the n values:"))
for i in range(n):
    for j in range(n):
        print("*",end=" ")
    print()

n=int(input("enetr the n values:"))
for i in range(n):
    for j in range(n):
        if i==0 or j==0 or i==n-1 or j==n-1: 
            print("*",end=" ")
        else:
            print(" ",end=" ")
    print()

